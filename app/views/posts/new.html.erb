<h1>Crate New Post</h1>

<%= form_with model: @post do |form| %>
  <div>
    <%= form.label :Title %><br>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :description %><br>
    <%= form.text_field :description %>
  </div>  

  <div>
    <%= form.label :image %><br>
    <%= form.file_field :image %>
  </div> 
  <div>
    <%= form.submit %>
  </div>
<% end %>
